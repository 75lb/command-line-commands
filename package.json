{
  "name": "command-line-commands",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "1.0.4",
  "description": "Add a git-like command interface to your app.",
  "repository": "https://github.com/75lb/command-line-commands.git",
  "license": "MIT",
  "main": "index",
  "keywords": [
    "argv",
    "parse",
    "argument",
    "args",
    "option",
    "parser",
    "parsing",
    "cli",
    "command",
    "commands",
    "line"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "test-runner --files test/*.js",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs lib/*.js > README.md; echo",
    "es5": "babel --presets babel-preset-es2015 --no-comments lib --out-dir es5",
    "cover": "istanbul cover ./node_modules/.bin/test-runner -- --files test/*.js && cat coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf coverage; echo"
  },
  "dependencies": {
    "array-back": "^1.0.3",
    "feature-detect-es6": "^1.3.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "command-line-args": "^3.0.1",
    "command-line-usage": "^3.0.3",
    "core-assert": "^0.2.0",
    "coveralls": "^2.11.12",
    "jsdoc-to-markdown": "^2.0.0-alpha.12",
    "minimist": "^1.2.0",
    "test-runner": "^0.2.3"
  },
  "standard": {
    "ignore": [
      "es5"
    ]
  }
}
